{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts"
  ]
}
